{
    "imports": {
      "@concepts/": "./src/concepts/",
      "@concepts": "./src/concepts/concepts.ts",
      "@test-concepts": "./src/concepts/test_concepts.ts",
      "@utils/": "./src/utils/",
      "@engine": "./src/engine/mod.ts",
      "@syncs": "./src/syncs/syncs.ts"
    },
    "tasks": {
      "cache": "deno cache --reload src/main.ts && deno cache --reload src/concepts/Authentication/AuthenticationConcept.ts && deno run --allow-read warmup_bcrypt.ts",
      "cache-quick": "deno cache src/main.ts && deno cache src/concepts/Authentication/AuthenticationConcept.ts && deno run --allow-read warmup_bcrypt.ts",
      "start": "deno run --allow-net --allow-write --allow-read --allow-sys --allow-env src/main.ts",
      "import": "deno run --allow-read --allow-write --allow-env src/utils/generate_imports.ts",
      "build": "deno run import",
      "warm-bcrypt": "deno run warmup_bcrypt.ts"
    },
    "lint": {
      "rules": {
        "exclude": [
          "no-import-prefix",
          "no-unversioned-import"
        ]
      }
    }
  }
  